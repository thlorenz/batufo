{
  "name": "@batufo/server",
  "version": "0.0.0",
  "description": "Socket.io server for batufo game.",
  "main": "app.js",
  "scripts": {
    "tsc": "tsc",
    "watch": "tsc --watch",
    "start": "DEBUG_COLORS=1 DEBUG='(app|stats|game*):(debug|info|error)' node ./dist/app.js",
    "start:dev": "DEBUG_COLORS=1 DEBUG='(app|stats|game*):(debug|info|error)' ts-node ./src/app.ts"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@batufo/core": "*",
    "debug": "^4.1.1",
    "google-protobuf": "^3.12.0",
    "socket.io": "^2.3.0",
    "supports-color": "^7.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/google-protobuf": "^3.7.2",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "ts-protoc-gen": "^0.12.0"
  }
}
